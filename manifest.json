{
  "name": "USDT Ticker",
  "version": "0.0.1",
  "description": "USDT to INR ticker",
  "manifest_version": 2,
  "background": {
    "page": "src/background.html",
    "persistent": false
  },
  "icons": {
    "16": "static/icon16.png",
    "48": "static/icon48.png",
    "128": "static/icon128.png"
  },
  "browser_action": {
    "default_icon": "static/icon.png",
    "default_title": "USDT Ticker"
  },
  "permissions": ["alarms"]
}
